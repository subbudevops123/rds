CREATE DATABASE BookDB;



CREATE TABLE IF NOT EXISTS books (
    book_id INT AUTO_INCREMENT,
	title VARCHAR(255) NOT NULL,
	author VARCHAR(255),
	publish_date DATE,
	description TEXT,
	PRIMARY KEY (book_id)
)  ENGINE=INNODB;

